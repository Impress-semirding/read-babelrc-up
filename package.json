{
  "name": "read-babelrc-up",
  "version": "0.2.0",
  "description": "Read the closet .babelrc file",
  "license": "MIT",
  "repository": "akameco/read-babelrc-up",
  "author": {
    "name": "akameco",
    "email": "akameco.t@gmail.com",
    "url": "akameco.github.io"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "add-contributor": "all-contributors add",
    "precommit": "lint-staged",
    "fmt": "prettier --write '*.{js,json,md}'",
    "test": "eslint . && ava"
  },
  "files": ["index.js"],
  "lint-staged": {
    "*.js": ["fmt", "test", "git add"],
    "*.json": ["fmt", "git add"],
    "*.md": ["fmt", "git add"]
  },
  "keywords": [
    "babel",
    "babelrc",
    ".babelrc",
    "json",
    "file",
    "parse",
    "load",
    "look-up",
    "walk",
    "path"
  ],
  "dependencies": {
    "find-up": "^2.1.0",
    "json5": "^0.5.1"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.10.0",
    "ava": "^0.24.0",
    "eslint": "^4.13.1",
    "eslint-config-precure": "^0.11.3",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2"
  }
}
